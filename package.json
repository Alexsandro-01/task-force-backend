  },
  "homepage": "https://github.com/Alexsandro-01/task-force-backend#readme",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.3",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.3",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "^10.0.13",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.6",
