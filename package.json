    "dotenv": "^16.0.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.0.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.1",
